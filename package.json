{
  "name": "luggage_app",
  "version": "1.0.0",
  "description": "Luggage Identifying System",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode production --open --hot",
    "server": "nodemon --exec babel-node server/index.js",
    "client": "webpack-dev-server --open --mode development --hot --watch",
    "dev:server": "concurrently --kill-others \"npm run server\" \"npm run client\"",
    "test:client": "jest -u",
    "test:watch": "jest --watch",
    "test:server": "NODE_ENV=test nyc mocha --recursive --require @babel/register ./server/tests/ --exit",
    "coverage:server": "nyc --reporter=lcov --reporter=text --report-dir ./server/coverage npm run test:server",
    "coverage:client": "jest --collectCoverage",
    "lint": "eslint"
  },
  "author": "Victor Nomwesigwa",
  "license": "ISC",
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      "node_modules/**/node_modules"
    ],
    "delay": "2500",
    "env": {
      "NODE_ENV": "development",
      "PORT": 4000
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.7.4",
    "@babel/node": "^7.7.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-transform-async-to-generator": "^7.5.0",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.7.4",
    "@babel/register": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "concurrently": "^5.0.0",
    "css-loader": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.8.0",
    "moxios": "^0.4.0",
    "nodemon": "^1.19.2",
    "path": "^0.12.7",
    "prettier": "^1.18.2",
    "react-hot-loader": "^4.12.15",
    "react-test-renderer": "^16.11.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.0.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2"
  },
  "dependencies": {
    "@babel/preset-react": "^7.7.4",
    "axios": "^0.19.0",
    "babel-cli": "^7.0.0-0",
    "babel-node": "0.0.1-security",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.3.1",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "eslint": "^6.4.0",
    "eslint-config-airbnb": "^18.0.1",
    "express": "^4.17.1",
    "joi": "^14.3.1",
    "jsonwebtoken": "^9.0.0",
    "mocha": "^6.2.0",
    "mongoose": "^5.7.1",
    "node-sass": "^4.13.0",
    "nodemailer": "^6.3.0",
    "nyc": "^14.1.1",
    "react": "^16.11.0",
    "react-bootstrap": "^1.0.0-beta.14",
    "react-bootstrap-button-loader": "^2.0.0",
    "react-dom": "^16.11.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^2.1.8",
    "react-toastify": "^5.4.1",
    "resolve-url-loader": "^3.1.0",
    "sass-loader": "^8.0.0",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.3.0",
    "sinon-express-mock": "^2.2.1",
    "supertest": "^4.0.2",
    "url-loader": "^2.2.0"
  }
}
